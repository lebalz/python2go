{
	"name": "python2go",
	"displayName": "python2go",
	"publisher": "lebalz",
	"description": "install and configure python to start straight away",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.46.0"
	},
	"homepage": "https://github.com/lebalz/python2go/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/lebalz/python2go/issues"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lebalz/python2go"
	},
	"galleryBanner": {
		"color": "#ffffff",
		"theme": "light"
	},
	"icon": "logo.png",
	"categories": [
		"Other"
	],
	"extensionPack": [
		"ms-python.python"
	],
	"activationEvents": [
		"onCommand:python2go.install",
		"onCommand:python2go.configure",
		"onCommand:python2go.uninstall"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "python2go.install",
				"title": "Python2Go: Install"
			},
			{
				"command": "python2go.configure",
				"title": "Python2Go: Configure"
			},
			{
				"command": "python2go.uninstall",
				"title": "Python2Go: Uninstall"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/node-powershell": "^3.1.1",
		"node-powershell": "^4.0.0"
	}
}
